		<canvas id="myCanvas" class="awesome-canvas"></canvas>
		<script src="../scripts/canvas.js"></script>

		<script>
			//@ts-ignore
			window.dataLayer = window.dataLayer || [];
			function gtag() {
				//@ts-ignore
				dataLayer.push(arguments);
			}
			//@ts-ignore
			gtag("js", new Date());
			//@ts-ignore
			gtag("config", "G-FBBE6W9PNR");
		</script>
