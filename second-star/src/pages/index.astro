---
export interface Props {
	title: string;
}

import "bootstrap/dist/css/bootstrap.css";
import "../../public/css/style.scss";
import "../../public/css/font-awesome.min.css";
import "@fontsource/signika-negative";
import "@fontsource/kalam";

import { SEO } from "astro-seo";

import Heading from "../components/heading.astro";
import Footer from "../components/footer.astro";
import Skills from "../components/skills.astro";
import Perks from "../components/perks.astro";
import Activity from "../components/activity.astro";
import WorkHistory from "../components/work-history.astro";
import Expertise from "../components/expertise.astro";
import WorkExperience from "../components/work-experience.astro";
import References from "../components/references.astro";
import Cursor from "../components/cursor.astro";
---

<html lang="en">
	<head>
		<SEO
